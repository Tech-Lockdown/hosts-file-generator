{
  "name": "hosts-file-generator",
  "version": "1.0.0",
  "description": "generate hosts file based on crowdsourced categories",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll --notify",
    "test-server": "node tests/test.server.js",
    "start": "node app.js",
    "generate:blocklist": "node cli.js --blocklist parentalcontrol,privacy,security",
    "generate:cache": "node cli.js --cache parentalcontrol,privacy,security",
    "get:blocklist": "node cli.js --getblocklist parentalcontrol,privacy,security",
    "get:options": "node cli.js --options parentalcontrol,privacy,security",
    "get:cachemap": "node cli.js --getcachemap",
    "set:cachemap": "node cli.js --setcachemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tech-Lockdown/hosts-file-generator.git"
  },
  "author": "Ben Bozzay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tech-Lockdown/hosts-file-generator/issues"
  },
  "homepage": "https://github.com/Tech-Lockdown/hosts-file-generator#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "multistream": "^4.1.0",
    "node-fetch": "^3.1.0",
    "path": "^0.12.7",
    "replacestream": "^4.0.3",
    "request": "^2.88.2",
    "tmp": "^0.2.1",
    "tmp-promise": "^3.0.3"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "babel-jest": "^27.4.2",
    "jest": "^27.4.3",
    "node-notifier": "^10.0.0",
    "regenerator-runtime": "^0.13.9"
  }
