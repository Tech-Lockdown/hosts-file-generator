  },
  "homepage": "https://github.com/Tech-Lockdown/hosts-file-generator#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "node-fetch": "^3.1.0"
  },
  "type": "module"
}
