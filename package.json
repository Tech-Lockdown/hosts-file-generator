{
  "name": "hosts-file-generator",
  "version": "1.0.0",
  "description": "generate hosts file based on crowdsourced categories",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "generate:blocklist": "node cli.js --blocklist parentalcontrol,privacy,security",
    "generate:cache": "node cli.js --cache parentalcontrol,privacy,security",
    "get:blocklist": "node cli.js --getblocklist parentalcontrol,privacy,security",
    "get:options": "node cli.js --options parentalcontrol,privacy,security",
    "get:cachemap": "node cli.js --getcachemap",
    "set:cachemap": "node cli.js --setcachemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tech-Lockdown/hosts-file-generator.git"
  },
